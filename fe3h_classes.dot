          width=1.55,
          shape=box,
          style=filled,
          fillcolor=white ];
    //black eagles
    node [color="#B5433C", fillcolor="#ff000045"];
    Edelgard[label="エーデルガルト"];
    Hubert[label="ヒューベルト"];
    Ferdinand[label="フェルディナント"];
    Linhardt[label="リンハルト"];
    Caspar[label="カスパル"];
    Bernadetta[label="ベルナデッタ"];
    Dorothea[label="ドロテア"];
